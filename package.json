{
  "name": "project-manager",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec \"npx tsx server.ts\" --watch server.ts --watch src --watch config --ext ts,tsx,js,jsx",
    "dev:network": "NODE_ENV=development nodemon --exec \"npx tsx server.ts\" --watch server.ts --watch src --watch config --ext ts,tsx,js,jsx",
    "dev:windows": "set NODE_ENV=development && nodemon --exec \"npx tsx server.ts\" --watch server.ts --watch src --watch config --ext ts,tsx,js,jsx",
    "dev:mac": "NODE_ENV=development nodemon --exec 'npx tsx server.ts' --watch server.ts --watch src --watch config --ext ts,tsx,js,jsx",
    "dev:force": "NODE_ENV=development FORCE_DEV=true nodemon --exec \"npx tsx server.ts\" --watch server.ts --watch src --watch config --ext ts,tsx,js,jsx",
    "build": "NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "postinstall": "prisma generate",
    "deploy": "npm ci --production=false && npm run build",
    "start": "cross-env NODE_ENV=production tsx server.ts",
    "start:windows": "set NODE_ENV=production && tsx server.ts",
    "start:mac": "NODE_ENV=production tsx server.ts",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts"
  },
